{
  "cli_tools": {
    "claude": {
      "name": "Claude Code",
      "start_command": "claude"
    },
    "kimi": {
      "name": "Kimi CLI",
      "start_command": "kimi"
    },
    "opencode": {
      "name": "OpenCode",
      "start_command": "opencode"
    }
  },
  "roles": {
    "orchestrator": { "cli": "claude" },
    "project_leader": { "cli": "claude" },
    "executor": { "cli": "claude" },
    "project_manager": { "cli": "claude" },
    "developer": { "cli": "kimi" },
    "qa": { "cli": "opencode" },
    "devops": { "cli": "claude" },
    "code_reviewer": { "cli": "claude" },
    "researcher": { "cli": "opencode" },
    "documentation_writer": { "cli": "kimi" }
  },
  "defaults": {
    "cli": "claude",
    "executors": 1,
    "state_dir": "~/.tmux-orchestrator"
  }
}
